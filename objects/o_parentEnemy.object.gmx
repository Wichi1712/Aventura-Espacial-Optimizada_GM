          <argument>
            <kind>1</kind>
            <string>///Acciones segun enemigo
//event_inherited();
if(tipoEnemigo=="nave"){
    if( distance_to_object( balaEnemigo)&gt;20){
        instance_create(x, y + 20, balaEnemigo);
        balaEnemigo.vspeed = 7;
    }
}

if(tipoEnemigo=="medusa"){
    if( distance_to_object( balaEnemigo)&gt;30){
        instance_create(x, y + 10, balaEnemigo);
        balaEnemigo.vspeed = 2;
    }
}

if(tipoEnemigo=="roca"){
    image_angle+=rotacion;
}

if(tipoEnemigo=="jefe"){
    //--------Movimiento horizontal continuo------
    /*Cambia de direccion al llegar al borde horizontal de la room*/
    if( y &gt;= 50){
        if( x &gt; room_width){velocidad = -2;}//izquierda
        else if( x &lt; 0){velocidad = 2;}//derecha
            
        x = x + velocidad;
    }

    switch (estadoJefe)
    {
        case "inicio":
        //Se detiene cuando y es menor o igual a 50
        if( y &lt;= 50){y+=1;}
        else{y = 50; estadoJefe = "ataque";}
        break;
        
        case "ataque":  
        //-----------Disparo--------------------------
        if( distance_to_object( balaEnemigo)&gt;30){
            instance_create(x, y + 30, balaEnemigo);
            balaEnemigo.vspeed = 2;
        }
        
        //---------control resistencia------------
        if( resistencia &lt;= 80)estadoJefe = "ataque2";
        
        break;
        
        case "ataque2":
        image_blend = c_olive;

        //-----------Disparo--------------------------
        if( distance_to_object( balaEnemigo)&gt;30){
            instance_create(x, y + 30, balaEnemigo);
            balaEnemigo.vspeed = 5;
        }
        
        //---------control resistencia------------
        if( resistencia &lt;= 0)estadoJefe = "eliminado";
        break;
        
        case "eliminado":
        image_blend = c_gray;
        instance_create( random_range(x-15, x+15),random_range(y-20, y+20), o_explosion);
        y+=1;
        break;
    }
}
