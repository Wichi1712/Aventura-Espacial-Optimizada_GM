            <string>///Acciones  de score segun enemigo
/*
    *Aqui cada tipo de enemigo tiene un valor de score diferente.
    *Con respecto a los enemigos roca y jefe solo se destruye si
     su resistencia llega a cero.
     
     *************TABLA DE SCORE*************************
     Roca------------------&gt;5 Pts.
     Nave------------------&gt;10 Pts.
     Medusa----------------&gt;15 Pts.
     Jefe------------------&gt;100 Pts.
*/

//Almacenamos en la variable pts una instancia del o_textoFlotante
var pts = instance_create(x,y, o_textoFlotante);

if(tipoEnemigo == "nave"){
    score +=valorScore;
    pts.txt = string( valorScore);//crea o_textoFlotante y le asigna un valor
}
else
if(tipoEnemigo == "medusa"){
    score +=valorScore;
    pts.txt = string( valorScore);
}
else
if(tipoEnemigo == "roca"){
    resistencia--;
    rotacion=choose(-5,5);
    if(resistencia &lt;= 0){
        score +=valorScore;
        pts.txt = string( valorScore);
    }
}
else
if(tipoEnemigo == "jefe" &amp;&amp; estadoJefe != "eliminado"){
    resistencia--;
    if(resistencia &lt;= 0){
        score +=valorScore;
        pts.txt = string( valorScore);
    }
}
